{
  "sourceMaps": true,
  "presets": [
    "@babel/env",
    "@babel/react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import",
    ["module-resolver", {
      "root": ["./src"]
    }],
    ["import",{"libraryName":"antd","libraryDirectory":"es","style":"css"}]
  ]
}
